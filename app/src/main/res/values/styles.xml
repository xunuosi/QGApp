<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:windowContentTransitions">true</item>
    </style>

    <style name="NoTitleTheme" parent="AppTheme">

    </style>

    <style name="tool_bar">
        <item name="android:textColor">@color/color_toolbar_text</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="login_edit_text">
        <item name="android:textColorHint">@color/color_login_gray</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="register_text">
        <item name="android:textColor">@color/color_register_text_gray</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginEnd">15dp</item>
    </style>

    <style name="register_edit_text">
        <item name="android:textColorHint">@color/color_login_gray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@null</item>
    </style>
    <!--HomeFragment-->
    <style name="hf_store_text">
        <item name="android:textColor">@color/color_home_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="hf_title_text">
        <item name="android:textColor">@color/color_hf_title_text</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="hf_banner_title_text">
        <item name="android:textColor">@color/color_white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/title_bg_blue</item>
    </style>

    <style name="hf_banner_center_title_text">
        <item name="android:textColor">@color/color_white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="hf_standard_title_text">
        <item name="android:textColor">@color/color_hf_stad_title_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="hf_standard_text">
        <item name="android:textColor">@color/color_hf_stad_text</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="hf_nb_text">
        <item name="android:textColor">@color/color_home_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="hf_article_text_title">
        <item name="android:textColor">@color/color_home_text</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="hf_article_text_content">
        <item name="android:textColor">@color/color_hf_article_content_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="hf_article_text_info">
        <item name="android:textColor">@color/color_hf_article_info_text</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">left</item>
    </style>
    <!--KnowledgeFragment-->
    <style name="kf_text_title" parent="hf_article_text_title"/>

    <style name="kf_text_info" parent="hf_article_text_content"/>
    <!--DBDetail Activity-->
    <style name="db_detail_text" parent="hf_standard_text"/>
    <!--DBRes Activity-->
    <style name="db_res_text_title" parent="hf_article_text_title"/>

    <style name="db_res_text_content" parent="hf_article_text_content"/>

    <style name="db_res_text_info" parent="hf_article_text_info"/>
    <!--PopMenu-->
    <style name="TRM_ANIM_STYLE">
        <item name="android:windowEnterAnimation">@anim/trm_popup_enter</item>
        <item name="android:windowExitAnimation">@anim/trm_popup_exit</item>
    </style>
    <!--TreeView Style-->
    <style name="TreeNodeStyleCustom">
        <item name="android:paddingLeft">20dp</item>
    </style>

    <style name="TreeNodeStyleDivided">
        <item name="android:showDividers">middle</item>
        <item name="android:divider">@android:drawable/divider_horizontal_bright</item>
    </style>
    <!--DBRes Word-->
    <style name="db_res_word_text_title">
        <item name="android:textColor">@color/color_home_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="db_res_word_text_info">
        <item name="android:textColor">@color/color_login_gray</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <!--KDB Dic Detail Activity-->
    <style name="kdb_dic_detail_text_title">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/color_selected_bottom_text</item>
    </style>

    <style name="kdb_dic_detail_text_info" parent="hf_standard_text"/>

    <style name="kdb_dic_detail_text_content" parent="hf_standard_text">
        <item name="android:textSize">16sp</item>
    </style>
    <!--KDB Book Detail Activity-->
    <style name="kdb_book_detail_text_title">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/color_home_text</item>
    </style>

    <style name="my_RatingBar" parent="@android:style/Widget.Material.RatingBar.Small">
        <item name="android:progressDrawable">@drawable/rating_bg</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:maxHeight">16dp</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/color_login_blue</item>
        <item name="colorControlActivated">@color/google_red</item>
    </style>

    <style name="kdb_book_detail_text_book_info" parent="hf_standard_text"/>

    <style name="book_fragment_text">
        <item name="android:textColor">@color/color_hf_article_info_text</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="book_fragment_blue_text" parent="book_fragment_text">
        <item name="android:textColor">@color/color_login_blue</item>
    </style>

    <style name="book_fragment_content_text" parent="book_fragment_text"/>
    <!--My Center Fragment-->
    <style name="my_center_fragment_head_text">
        <item name="android:textColor">@color/color_white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="my_center_fragment_head_indicator">
        <item name="android:textColor">@color/color_white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="my_center_fragment_text_content">
        <item name="android:textColor">@color/color_my_center_tag</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <!--User Home-->
    <style name="user_home_text_gray">
        <item name="android:textColor">@color/color_hf_article_content_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="user_home_text_black">
        <item name="android:textColor">@color/color_hf_stad_title_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <!--Res DataBase-->
    <style name="dbres_text_blue">
        <item name="android:textColor">@color/color_text_blue</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical|right</item>
    </style>

    <style name="dbres_text_title" parent="hf_article_text_title">
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="dbres_text_content" parent="dbres_text_title">
        <item name="android:textColor">@color/color_common_indicator</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="dbres_text_indicator" parent="dbres_text_title">
        <item name="android:textColor">@color/color_common_content</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!--Video-->
    <style name="video_set_title">
        <item name="android:textColor">@color/color_white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="ExoMediaButton.Full">
        <item name="android:src">@drawable/full_screen</item>
        <item name="android:contentDescription">@string/exo_controls_full_screen_description</item>
    </style>
    <!--Read-->
    <style name="read_footer">
        <item name="android:textColor">@color/mask_color</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:textSize">18sp</item>
    </style>
    <!--ebook-->
    <style name="ebook_item_price">
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:textColor">@color/color_hf_article_info_text</item>
    </style>
    <style name="ebook_item_price_red" parent="ebook_item_price">
        <item name="android:textColor">@color/color_red</item>
    </style>
    <!--cook-->
    <style name="cook_info_item_title" parent="kdb_book_detail_text_title">
        <item name="android:gravity">center</item>
    </style>
    <!--Master-->
    <style name="master_info_text_blue">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/color_login_blue</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="master_info_text">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/color_hf_article_info_text</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="master_info_text_gray">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/mask_color</item>
        <item name="android:gravity">left</item>
    </style>

    <!-- selectable TextView -->
    <style name="OperateTextView">
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:textColor">@color/color_hf_article_info_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_gray_rect_selector</item>
    </style>
</resources>
